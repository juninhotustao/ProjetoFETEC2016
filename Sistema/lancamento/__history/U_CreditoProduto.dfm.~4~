  object DTS: TSQLDataSet
    SchemaName = 'sa'
    GetMetadata = False
    CommandText = 
      'SELECT'#13#10#9'ITV_ID, PRO_REFERENCIA, PRO_DESCRICAO, ITV_QTDE'#13#10'FROM'#13#10 +
      #9'ITEM_VENDA I'#13#10'INNER JOIN'#13#10#9'PRODUTO P ON I.PRO_ID = P.PRO_ID'#13#10'WH' +
      'ERE'#13#10#9'VEN_ID = :VEN_ID'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'VEN_ID'
        ParamType = ptInput
      end>
    SQLConnection = DMRet.Con
    Left = 373
    Top = 118
    object DTSITV_ID: TIntegerField
      FieldName = 'ITV_ID'
      ProviderFlags = [pfInWhere, pfInKey]
    end
    object DTSPRO_REFERENCIA: TStringField
      FieldName = 'PRO_REFERENCIA'
      ProviderFlags = []
