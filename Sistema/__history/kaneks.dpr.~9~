program kaneks;

uses
  Vcl.Forms,
  U_Principal in 'U_Principal.pas' {frm_Principal},
  U_Login in 'U_Login.pas' {frm_Login},
  U_CadastroBase in 'cadastro\U_CadastroBase.pas' {Frm_CadastroBase},
  U_ConsultaBase in 'cadastro\U_ConsultaBase.pas' {Frm_ConsultaBase},
  U_DMRet in 'devartDAL\U_DMRet.pas' {DMRet: TDataModule},
  U_FuncUtils in 'biblioteca\U_FuncUtils.pas',
  U_ConClientes in 'cadastro\Cadastro\U_ConClientes.pas' {frm_ConClientes},
  U_CadClientes in 'cadastro\Cadastro\U_CadClientes.pas' {frm_CadClientes},
  U_CadFornecedor in 'cadastro\Cadastro\U_CadFornecedor.pas' {frm_CadFornecedor},
  U_ConFornecedor in 'cadastro\Cadastro\U_ConFornecedor.pas' {frm_ConFornecedor};

{$R *.res}

