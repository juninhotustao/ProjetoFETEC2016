program kaneks;

uses
  Vcl.Forms,
  U_Principal in 'U_Principal.pas' {frm_Principal},
  U_Login in 'U_Login.pas' {frm_Login},
  U_CadastroBase in 'cadastro\U_CadastroBase.pas' {Frm_CadastroBase},
  U_ConsultaBase in 'cadastro\U_ConsultaBase.pas' {Frm_ConsultaBase},
  U_DMRet in 'devartDAL\U_DMRet.pas' {DMRet: TDataModule},
  U_FuncUtils in 'biblioteca\U_FuncUtils.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(Tfrm_Principal, frm_Principal);
  Application.CreateForm(Tfrm_Login, frm_Login);
  Application.CreateForm(TFrm_CadastroBase, Frm_CadastroBase);
  Application.CreateForm(TFrm_ConsultaBase, Frm_ConsultaBase);
  Application.CreateForm(TDMRet, DMRet);
  Application.Run;
end.
